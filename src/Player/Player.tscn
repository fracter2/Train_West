resource_name = "Indicator_Gun_Tool"
script/source = "extends ColorRect

var color_gun:Color = Color(0.929412, 0.411765, 0.066667)
var color_tool:Color = Color(0.168627, 0.909804, 0.839216)
var state:bool = false

func _ready():
	color = Color(0.929412, 0.411765, 0.066667)


func toggle():
	state = not state
	
	if state == false:
		$Label_Gun.visible = true
		$Label_Tool.visible = false
		color = color_gun
	else:
		$Label_Gun.visible = false
		$Label_Tool.visible = true
		color = color_tool
"

